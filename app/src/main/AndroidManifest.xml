<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bombiahorro"
    android:installLocation="auto"
    android:versionCode="001"
    android:versionName="BETA">

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />
    <application
        android:allowBackup="true"
        android:configChanges="orientation|screenSize"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:logo="@drawable/app_icon"
        android:theme="@style/AppTheme">
        <activity android:name="activities.Menu">
            <intent-filter>
                <action android:name="com.example.bombiahorro.MENU" />
            </intent-filter>
        </activity>
        <activity android:name="activities.MainActivity">
        </activity>
        <activity
            android:name="activities.ScreenLoaderActivity"
            android:noHistory="true"
            android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>